<?xml version="1.0" encoding="UTF-8"?>
<!--
  DTD for KyrreML - hovudformatet for data som foelgjer Kyrre - Standard for dataregistrering av kyrkjeboeker i Noreg
  
  Versjon 1.0, 04.08.2005 (utarbeidd av Lars Nygaard, Riksarkivet)
-->
<!-- Overordna element: -->
<grammar xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://relaxng.org/ns/structure/1.0" 
    datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
    
    <include href="generelt.rng"/>
    <start combine="choice">
        <ref name="kyrre"></ref>
    </start>
<define name="kyrre">
    <element name="kyrre">
      <ref name="attlist.kyrre"/>
      <ref name="kjelde"/>
      <ref name="registrering"/>
      <zeroOrMore>
        <ref name="dp"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="kf"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="vi"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="gr"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="df"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="va"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="if"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="uf"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="pa"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="ko"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="in"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="im"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="um"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.kyrre" combine="interleave">
    <optional>
      <attribute name="versjon" a:defaultValue="1.2">
        <value type="string">1.2</value>
      </attribute>
    </optional>
  </define>
  <!-- Versjonsnummer av KYRRE -->
  <!-- Felles underelement for fleire hovudelement: -->
  <define name="merknader"  combine="choice">
    <element name="merknader">
      <ref name="attlist.merknader"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.merknader" combine="interleave">
    <empty/>
  </define>
  <!-- Evt. merknader til kjelda, registreringa eller ei handling -->
  <define name="side" combine="choice">
    <element name="side">
      <ref name="attlist.side"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.side" combine="interleave">
    <empty/>
  </define>
  <!-- Sidetalet i kyrkjeboka -->
  <define name="lopenr">
    <element name="lopenr">
      <ref name="attlist.lopenr"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.lopenr" combine="interleave">
    <empty/>
  </define>
  <!-- Handlinga sitt loepenummer i kyrkjeboka, innafor aaret -->
  <define name="aar" combine="choice">
    <element name="aar">
      <ref name="attlist.aar"/>
      <data type="gYear"></data>
    </element>
  </define>
  <define name="attlist.aar" combine="interleave">
    <empty/>
  </define>
  <!-- Aaret handlinga vart foert i kyrkjeboka -->
  <define name="sokn_kyrkje">
    <element name="sokn_kyrkje">
      <ref name="attlist.sokn_kyrkje"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.sokn_kyrkje" combine="interleave">
    <empty/>
  </define>
  <!-- Namn paa sokn eller kyrkje for handlinga -->
  <define name="sokn">
    <element name="sokn">
      <ref name="attlist.sokn"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.sokn" combine="interleave">
    <empty/>
  </define>
  <!-- Namn paa sokn for verdsleg handling -->
  <define name="meldar">
    <element name="meldar">
      <ref name="attlist.meldar"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.meldar" combine="interleave">
    <empty/>
  </define>
  <!-- Opplysningar om melding av doedfoedsel -->
  <define name="rolle" combine="choice">
    <element name="rolle">
      <ref name="attlist.rolle"/>
      <ref name="rolleliste"/>
    </element>
  </define>
  <define name="attlist.rolle" combine="interleave">
    <empty/>
  </define>
  <!--
    Personen si rolle i handlinga. Lovlige verdiar: barn, far, mor,
    fadder, heimedoeypar, daapsvitne, daapsmeldar, konfirmant, brudgom,
    brur, brudgommens far, bruras far, trulovar, lysingsforlangar,
    avdoed, paaroerande, meldar, foedselshjelpar, vaksinert, vaksinatoer,
    innflyttar, utflyttar, syndar, kommunikant, introdusert, ektemann,
    innmeld, utmeld
  -->
  <define name="stilling_stand">
    <element name="stilling_stand">
      <ref name="attlist.stilling_stand"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.stilling_stand" combine="interleave">
    <empty/>
  </define>
  <!-- Personen si stilling eller stand viss oppgjeve i kjelda -->
  <define name="forenamn">
    <element name="forenamn">
      <ref name="attlist.forenamn"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.forenamn" combine="interleave">
    <empty/>
  </define>
  <!-- Personen sitt foerenamn -->
  <define name="etternamn">
    <element name="etternamn">
      <ref name="attlist.etternamn"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.etternamn" combine="interleave">
    <empty/>
  </define>
  <!-- Personen sitt etternamn -->
  <define name="kjonn">
    <element name="kjonn">
      <ref name="attlist.kjonn"/>
      <choice>
          <value>k</value>
          <value>m</value>
          <value>??</value>
          <value></value>
        </choice>
    </element>
  </define>
  <define name="attlist.kjonn" combine="interleave">
    <empty/>
  </define>
  <!-- Personen sitt kjoenn. Lovlige verdiar: m, k, ??, m!!, k!! -->
  <define name="bustad">
    <element name="bustad">
      <ref name="attlist.bustad"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.bustad" combine="interleave">
    <empty/>
  </define>
  <!-- Personen sin bustad eller opphaldsstad -->
  <define name="fodestad">
    <element name="fodestad">
      <ref name="attlist.fodestad"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.fodestad" combine="interleave">
    <empty/>
  </define>
  <!-- Personen sin foedestad -->
  <define name="fodselsdato">
    <element name="fodselsdato">
      <ref name="attlist.fodselsdato"/>
      <choice>
        <data type="gMonthDay"></data> <!--ISO-dato, er antaglig ikke brukt noe sted -->
        <data type="NMTOKEN">
            <param name="pattern">[0-3][0-9].[0-1][0-9].?[0-9]?[0-9]?[*]?</param>
        </data>
        </choice>
    </element>
  </define>
  <define name="attlist.fodselsdato" combine="interleave">
    <empty/>
  </define>
  <!-- Personen sin foedselsdato viss oppgjeve, format: dd.mm -->
  <define name="fodselsaar">
    <element name="fodselsaar">
      <ref name="attlist.fodselsaar"/>
      <data type="gYear"></data>
    </element>
  </define>
  <define name="attlist.fodselsaar" combine="interleave">
    <empty/>
  </define>
  <!-- Personen sitt foedselsaar viss oppgjeve, format: 4 siffer -->
  <define name="alder">
    <element name="alder">
      <ref name="attlist.alder"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.alder" combine="interleave">
    <empty/>
  </define>
  <!-- Personen sin alder viss oppgjeve -->
  <define name="ekte_uekte">
    <element name="ekte_uekte">
      <ref name="attlist.ekte_uekte"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.ekte_uekte" combine="interleave">
    <empty/>
  </define>
  <!-- Om barnet er ekte eller uekte foedd. Verdiar: E eller U -->
  <define name="vaksin">
    <element name="vaksin">
      <ref name="attlist.vaksin"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.vaksin" combine="interleave">
    <empty/>
  </define>
  <!--
    Opplysningar om konfirmanten, brudgommen eller brura sin koppe-
    vaksinasjon etc.
  -->
  <!-- Dokumentasjon om kyrkjeboka og den aktuelle lista i denne: -->
  <define name="kjelde">
    <element name="kjelde">
      <ref name="attlist.kjelde"/>
      <ref name="prgjeld"/>
      <optional>
        <ref name="ksokn"/>
      </optional>
      <ref name="kommnr"/>
      <optional>
        <ref name="soknenr"/>
      </optional>
      <ref name="ktype"/>
      <optional>
        <ref name="signatur"/>
      </optional>
      <ref name="ltype"/>
      <ref name="startaar"/>
      <ref name="sluttaar"/>
      <optional>
        <ref name="oppbstad"/>
      </optional>
      <optional>
        <ref name="merknader"/>
      </optional>
    </element>
  </define>
  <define name="attlist.kjelde" combine="interleave">
    <attribute name="kjeldeID">
        <text/>
</attribute>
  </define>
  <define name="prgjeld">
    <element name="prgjeld">
      <ref name="attlist.prgjeld"/>
      <text/>
    </element>
  </define>
  <define name="attlist.prgjeld" combine="interleave">
    <empty/>
  </define>
  <!-- Prestegjeldets eller byens namn -->
  <define name="ksokn">
    <element name="ksokn">
      <ref name="attlist.ksokn"/>
      <text/>
    </element>
  </define>
  <define name="attlist.ksokn" combine="interleave">
    <empty/>
  </define>
  <!-- Soknets eller kyrkjelydens namn -->
  <define name="kommnr">
    <element name="kommnr">
      <ref name="attlist.kommnr"/>
      <data type="NMTOKEN">
        <param name="pattern">[0-9]{4}</param>
      </data>
    </element>
  </define>
  <define name="attlist.kommnr" combine="interleave">
    <empty/>
  </define>
  <!-- Kommunenummer til prestegjeldet eller soknet -->
  <define name="soknenr">
    <element name="soknenr">
      <ref name="attlist.soknenr"/>
      <text/>
    </element>
  </define>
  <define name="attlist.soknenr" combine="interleave">
    <empty/>
  </define>
  <!-- Soknenummer til prestegjeldet (6 siffer) eller soknet (8 s.) -->
  <define name="ktype"><!-- Kjeldetype ): MINI, KLOK, FREG, LYSN osb. -->
    <element name="ktype">
      <ref name="attlist.ktype"/>
      <choice>
          <value>MINI</value>
<value>KLOK</value>
<value>FREG</value>
<value>LYSN</value>
      </choice>
    </element>
  </define>
  <define name="attlist.ktype" combine="interleave">
    <empty/> 
  </define>
 
  <define name="signatur">
    <element name="signatur">
      <ref name="attlist.signatur"/>
      <text/>
    </element>
  </define>
  <define name="attlist.signatur" combine="interleave">
    <empty/>
  </define>
  <!-- Arkivsignatur ): referanse/nummer i Arkivverkets katalog -->
  <define name="ltype"> <!-- Listetype ): dp, kf, vi, gr, df, va, if, uf osb. -->
    <element name="ltype">
      <ref name="attlist.ltype"/>
      <ref name="liste.type"></ref>
    </element>
  </define>
  <define name="attlist.ltype" combine="interleave">
    <empty/>
  </define>
 
  <define name="startaar">
    <element name="startaar">
      <ref name="attlist.startaar"/>
        <data type="gYear">
            <param name="pattern">[1-2][5-9][0-9]{2}</param>
        </data>
    </element>
  </define>
  <define name="attlist.startaar" combine="interleave">
    <empty/>
  </define>
  <!-- Startaar for (kjelda eller) lista -->
  <define name="sluttaar">
    <element name="sluttaar">
      <ref name="attlist.sluttaar"/>
        <data type="gYear">
            <param name="pattern">[1-2][5-9][0-9]{2}</param>
        </data>
    </element>
  </define>
  <define name="attlist.sluttaar" combine="interleave">
    <empty/>
  </define>
  <!-- Sluttaar for (kjelda eller) lista -->
  <define name="oppbstad">
    <element name="oppbstad">
      <ref name="attlist.oppbstad"/>
      <text/>
    </element>
  </define>
  <define name="attlist.oppbstad" combine="interleave">
    <empty/>
  </define>
  <!-- Oppbevaringsstad for originalkjelda ): statsarkivkode e.a. -->
  <!-- Dokumentasjon om registreringa: -->
  <define name="registrering">
    <element name="registrering">
      <ref name="attlist.registrering"/>
      <optional>
        <ref name="reg_av"/>
      </optional>
      <optional>
        <ref name="reg_naar"/>
      </optional>
      <optional>
        <ref name="korr_av"/>
      </optional>
      <optional>
        <ref name="korr_naar"/>
      </optional>
      <ref name="foretak"/>
      <optional>
        <ref name="kontakt"/>
      </optional>
      <optional>
        <ref name="grunnlag"/>
      </optional>
      <optional>
        <ref name="program"/>
      </optional>
      <optional>
        <ref name="fildato"/>
      </optional>
      <optional>
        <ref name="merknader"/>
      </optional>
    </element>
  </define>
  <define name="attlist.registrering" combine="interleave">
    <empty/>
  </define>
  <define name="reg_av">
    <element name="reg_av">
      <ref name="attlist.reg_av"/>
      <text/>
    </element>
  </define>
  <define name="attlist.reg_av" combine="interleave">
    <empty/>
  </define>
  <!-- Namn paa person(er) som har registrert kjelda/lista -->
  <define name="reg_naar">
    <element name="reg_naar">
      <ref name="attlist.reg_naar"/>
      <text/>
    </element>
  </define>
  <define name="attlist.reg_naar" combine="interleave">
    <empty/>
  </define>
  <!-- Aar for eller anna tidfesting av registreringsarbeidet -->
  <define name="korr_av">
    <element name="korr_av">
      <ref name="attlist.korr_av"/>
      <text/>
    </element>
  </define>
  <define name="attlist.korr_av" combine="interleave">
    <empty/>
  </define>
  <!-- Namn paa person(er) som har korrekturlese kjelda/lista -->
  <define name="korr_naar">
    <element name="korr_naar">
      <ref name="attlist.korr_naar"/>
      <text/>
    </element>
  </define>
  <define name="attlist.korr_naar" combine="interleave">
    <empty/>
  </define>
  <!-- Aar for eller anna tidfesting av korrekturarbeidet -->
  <define name="foretak">
    <element name="foretak">
      <ref name="attlist.foretak"/>
      <text/>
    </element>
  </define>
  <define name="attlist.foretak" combine="interleave">
    <empty/>
  </define>
  <!--
    Organisasjon/institusjon eller person som staar bak
    registreringa; rettshavar
  -->
  <define name="kontakt">
    <element name="kontakt">
      <ref name="attlist.kontakt"/>
      <text/>
    </element>
  </define>
  <define name="attlist.kontakt" combine="interleave">
    <empty/>
  </define>
  <!-- Namn (evt.), adresse, telefon eller e-post til kontaktperson -->
  <define name="grunnlag">
    <element name="grunnlag">
      <ref name="attlist.grunnlag"/>
      <text/>
    </element>
  </define>
  <define name="attlist.grunnlag" combine="interleave">
    <empty/>
  </define>
  <!--
    Grunnlag for registreringa ): Papirkopi, Mikrofilm, Mikrofiche,
    Skanning eller Original
  -->
  <define name="program">
    <element name="program">
      <ref name="attlist.program"/>
      <text/>
    </element>
  </define>
  <define name="attlist.program" combine="interleave">
    <empty/>
  </define>
  <!-- Registreringsprogram ): Augustus, Access, Excel, BD87 osb. -->
  <define name="fildato">
    <element name="fildato">
      <ref name="attlist.fildato"/>
      <text/>
    </element>
  </define>
  <define name="attlist.fildato" combine="interleave">
    <empty/>
  </define>
  <!-- Dato for ferdigstilling av datafila, format: "dd.mm aaaa" -->
  <!-- Daap: Handling -->
  <define name="dp">
    <element name="dp">
      <ref name="attlist.dp"/>
      <ref name="side"/>
      <optional>
        <ref name="lopenr"/>
      </optional>
      <ref name="aar"/>
      <optional>
        <ref name="dpdato"/>
      </optional>
      <optional>
        <ref name="stadfesta"/>
      </optional>
      <optional>
        <ref name="dpstad"/>
      </optional>
      <optional>
        <ref name="introd"/>
      </optional>
      <optional>
        <ref name="melding"/>
      </optional>
      <optional>
        <ref name="sokn_kyrkje"/>
      </optional>
      <optional>
        <ref name="merknader"/>
      </optional>
      <oneOrMore>
        <ref name="dp_person"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.dp" combine="interleave">
    <optional>
      <attribute name="dpnr"/>
    </optional>
  </define>
  <!-- Handlingsnummer (daapsnummer) innafor fila -->
  <define name="dpdato">
    <element name="dpdato">
      <ref name="attlist.dpdato"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.dpdato" combine="interleave">
    <empty/>
  </define>
  <!-- Dato for daapen, anten i kyrkja eller heime, format: dd.mm -->
  <define name="stadfesta">
    <element name="stadfesta">
      <ref name="attlist.stadfesta"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.stadfesta" combine="interleave">
    <empty/>
  </define>
  <!-- Dato for evt. stadfesting i kyrkja, format: dd.mm -->
  <define name="dpstad">
    <element name="dpstad">
      <ref name="attlist.dpstad"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.dpstad" combine="interleave">
    <empty/>
  </define>
  <!-- Stad for daapen, anten i kyrkja eller heime -->
  <define name="introd">
    <element name="introd">
      <ref name="attlist.introd"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.introd" combine="interleave">
    <empty/>
  </define>
  <!-- Opplysningar om introduksjon av mora -->
  <define name="melding">
    <element name="melding">
      <ref name="attlist.melding"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.melding" combine="interleave">
    <empty/>
  </define>
  <!-- Opplysningar om melding av uekte barn -->
  <!-- Daap: Person -->
  <define name="dp_person">
    <element name="dp_person">
      <ref name="attlist.dp_person"/>
      <ref name="rolle"/>
      <optional>
        <ref name="stilling_stand"/>
      </optional>
      <optional>
        <ref name="forenamn"/>
      </optional>
      <optional>
        <ref name="etternamn"/>
      </optional>
      <ref name="kjonn"/>
      <optional>
        <ref name="bustad"/>
      </optional>
      <optional>
        <ref name="fodestad"/>
      </optional>
      <optional>
        <ref name="fodselsdato"/>
      </optional>
      <optional>
        <ref name="fodselsaar"/>
      </optional>
      <optional>
        <ref name="alder"/>
      </optional>
      <optional>
        <ref name="ekte_uekte"/>
      </optional>
      <optional>
        <ref name="vigd"/>
      </optional>
    </element>
  </define>
  <define name="attlist.dp_person" combine="interleave">
    <optional>
      <attribute name="pnr"/>
    </optional>
  </define>
  <!-- Personen sitt nummer innafor handlinga -->
  <define name="vigd">
    <element name="vigd">
      <ref name="attlist.vigd"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.vigd" combine="interleave">
    <empty/>
  </define>
  <!-- Foreldra sitt vigselsaar e.a. -->
  <!-- Konfirmasjon: Handling -->
  <define name="kf">
    <element name="kf">
      <ref name="attlist.kf"/>
      <ref name="side"/>
      <optional>
        <ref name="lopenr"/>
      </optional>
      <ref name="aar"/>
      <optional>
        <ref name="kfdato"/>
      </optional>
      <optional>
        <ref name="sokn_kyrkje"/>
      </optional>
      <optional>
        <ref name="merknader"/>
      </optional>
      <oneOrMore>
        <ref name="kf_person"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.kf" combine="interleave">
    <optional>
      <attribute name="kfnr"/>
    </optional>
  </define>
  <!-- Handlingsnummer (konfirmasjonsnummer) innafor fila -->
  <define name="kfdato">
    <element name="kfdato">
      <ref name="attlist.kfdato"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.kfdato" combine="interleave">
    <empty/>
  </define>
  <!-- Dato for konfirmasjonen, format: dd.mm -->
  <!-- Konfirmasjon: Person -->
  <define name="kf_person">
    <element name="kf_person">
      <ref name="attlist.kf_person"/>
      <ref name="rolle"/>
      <optional>
        <ref name="stilling_stand"/>
      </optional>
      <optional>
        <ref name="forenamn"/>
      </optional>
      <optional>
        <ref name="etternamn"/>
      </optional>
      <ref name="kjonn"/>
      <optional>
        <ref name="bustad"/>
      </optional>
      <optional>
        <ref name="fodestad"/>
      </optional>
      <optional>
        <ref name="fodselsdato"/>
      </optional>
      <optional>
        <ref name="fodselsaar"/>
      </optional>
      <optional>
        <ref name="alder"/>
      </optional>
      <optional>
        <ref name="doypt"/>
      </optional>
      <optional>
        <ref name="karakter"/>
      </optional>
      <optional>
        <ref name="vaksin"/>
      </optional>
    </element>
  </define>
  <define name="attlist.kf_person" combine="interleave">
    <optional>
      <attribute name="pnr"/>
    </optional>
  </define>
  <!-- Personen sitt nummer innafor handlinga -->
  <define name="doypt">
